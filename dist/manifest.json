{
    "manifest_version": 2,

    "name": "LeetCode2GitHub",
    "description":
        "Keep your leetcode solutions with you",
    "version": "0.0.1",

    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    },

    "background": {
        "scripts": ["js/eventPage.js"],
        "persistent": false
    },

    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },

    "permissions": [
        "https://api.github.com/*"
    ]
}
